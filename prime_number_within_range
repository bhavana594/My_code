#include<stdio.h>
bool isprime(int n)
{
	int count=0;	
	for(int i = 1; i <= n; i++)
	{
 		if(n%i==0)
 		count++;
	}
	if(count == 2)
	{
	 	return true;//return 1
	}
	else
	{
	 	return false;//return 0
	}
}
int main()
{
	
	int n,count =0;
	scanf("%d",&n);
	for(int i = 1; i<=n;i++)
	if(isprime(i))
		printf("%d ",i);
		count++;
	printf("\n%d",count);
	return 0;
}
